<h1>
Actors
</h1>



<% @list_of_actors.each do |actor| %>
<div class="container">
    <div class="row">
        <div class="col-sm-3">
        <h2><%= actor.name %></div>
        </h2>
        </div>
    </div>
    <div class="row">
        <ul><div class="col-sm-3"><img src="<%= actor.image_url %>" width="80"></ul>
        <ul><div class="col-sm-3">Date of Birth: <%= actor.dob %></div>
    </div></ul>
    <div class="row">
        <ul><div class="col-sm-12"><%= actor.bio %></div></ul>
    </div>
</div>


<% end %>
